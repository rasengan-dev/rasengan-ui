{
  "name": "Rasengan ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter @rasengan-ui/* dev",
    "build": "pnpm --filter @rasengan-ui/* build",
    "registry:build": "pnpm --filter @rasengan-ui/kit registry:build",
    "start": "pnpm --filter @rasengan-ui/* start",
    "lint": "pnpm -r lint",
    "test": "",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,mdx,css,html}\" --write \"**/*.json\" --write \"**/*.{yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,md,mdx,css,html}\" --check \"**/*.json\" --check \"**/*.{yml,yaml}\"",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=20.18.0"
  },
  "packageManager": "pnpm@8.10.0",
  "keywords": [],
  "author": "Dilane Kombou",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "husky": "^9.1.7",
    "prettier": "^3.7.4"
  }
}
